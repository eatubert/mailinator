{
  "name": "mailinator",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint",
    "sstdev": "test -n \"$AWS_PROFILE\" && npx sst dev || echo \"Error: AWS_PROFILE environment variable is required\"",
    "sstprod": "test -n \"$AWS_PROFILE\" && npx sst deploy --stage=production || echo \"Error: AWS_PROFILE environment variable is required\"",
    "start": "next start"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.958.0",
    "@aws-sdk/client-s3": "^3.958.0",
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "postal-mime": "^2.7.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sst": "3.17.25"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
